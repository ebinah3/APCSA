import static java.lang.System.*;
import java.util.Scanner;

public class Lab11j
{
   public static void main(String args[])
   {
	   Triples t = new Triples();
	   int input;
	   Scanner s = new Scanner(System.in);
	   while(true){
		   System.out.println("Enter number");
		   input = s.nextInt();
		   t.setNum(input);
		   t.findSets();
		   System.out.println(t);
	   }
	}
}